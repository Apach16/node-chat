{
    "presets": [
        "stage-0",
        [
            "env",
            {
                "modules": false
            }
        ],
        "node"
    ],
    "plugins": [
        "transform-decorators-legacy",
        "transform-class-properties",
        [
            "prismjs", {
                "languages": ["clike", "javascript", "typescript", "java", "c", "cpp", "python", "ruby", "markup", "markup-templating", "php", "go", "csharp", "css", "markdown", "sql", "json" ],
                "plugins": ["line-numbers"],
                "theme": "default",
                "css": true
            }
        ]
    ],
    "comments": false,
    "env": {
        "test": {
            "plugins": [
                "istanbul"
            ]
        },
        "development": {
            "plugins": [ ]
        }
    }
}